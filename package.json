{
  "name": "youtube-timestamps",
  "version": "1.1.0",
  "description": "A Chrome Extension for saving labelled timestamps of YouTube videos",
  "author": "Brian Lin <brianlin.software@gmail.com>",
  "homepage": "https://github.com/brianlinUM/youtube-timestamps#readme",
  "bugs": "https://github.com/brianlinUM/youtube-timestamps/issues",
  "keywords": [
    "YouTube",
    "video",
    "timestamp",
    "label",
    "bookmark"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brianlinUM/youtube-timestamps.git"
  },
  "main": "src/popup.js",
  "private": true,
  "scripts": {
    "dev": "webpack --config webpack.dev.js",
    "prod": "webpack --config webpack.prod.js",
    "test": "jest"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "vue",
      "json"
    ],
    "modulePaths": [
      "/src"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.html?$": "html-loader-jest",
      "^.+\\.vue$": "vue-jest"
    }
  },
  "dependencies": {
    "vue": "^2.6.12",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@testing-library/vue": "^5.6.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.0",
    "html-loader-jest": "^0.2.1",
    "jest": "^26.6.3",
    "jest-chrome": "^0.7.0",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0",
    "webpack-merge": "^5.8.0"
  }
}
